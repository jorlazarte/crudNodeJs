
	<main>
	<h1>PRODUCTOS</h1>

	<table>
		<thead>
			<tr>
				<th>Título</th>
				<th>Descripción</th>
			</tr>
		</thead>
		<tbody>
			<% products.forEach(product => { %>

			<tr>
				<td><%= product.title %></td>
				<td><%= product.description %></td>
			</tr>

			<% }); %>
		</tbody>
	</table>
	</main>
	
